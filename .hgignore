# Use regexp syntax to define patterns of non-vcs files.
syntax: regexp

# Exclude files beginning with "~"
([\\/]~|^~)

# Exclude IDEA project files
\.idea$
\.iml$
\.ipr$
\.iws$
\.ids$

# Exclude Eclipse project files
\.classpath$
\.project$
\.settings$

# Exclude Netbeans project files
([\\/]nb|^nb)project$

# Exclude build targets
([\\/]o|^o)ut$
([\\/]t|^t)arget$

# Exclude other resources
\.orig
\.log.*
release\.properties$
\.releaseBackup$
